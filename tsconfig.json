{
	"compilerOptions": {
		"target": "es2022",
		"lib": ["ESNext"],
		"moduleDetection": "force",

		"module": "preserve",
		"resolveJsonModule": true,
		"allowJs": true,
		"strict": true,
		// "noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
		"noEmit": true,
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
	}
}